{
  "name": "ygo",
  "version": "1.0.0",
  "description": "Card game",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "browserify": "^16.2.3",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript-formatter": "^7.2.2"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "build": "./node_modules/.bin/tsc -p src && ./node_modules/.bin/browserify build/frontend/index.js -o build/front-end.js",
    "watch": "./node_modules/.bin/tsc -w -p src",
    "format": "find src/ test/ -regex '.*.ts' | xargs ./node_modules/.bin/tsfmt -r"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TyRoXx/ygo.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TyRoXx/ygo/issues"
  },
  "homepage": "https://github.com/TyRoXx/ygo#readme"
}
